// * * * * * * * * * * * * * * Default settings  * * * * * * * * * * * * * * //
defaults
{
    standard
    {
        convergence     1e-2;
        minErrorScale   1e-6;
    }
    implicit
    {
        convergence     1e-2;
        minErrorScale   1e-6;
    }
}

